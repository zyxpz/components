<!DOCTYPE html>
<html lang="zh">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title><%= htmlWebpackPlugin.options.title  %></title>
  <style>
    @font-face {
      font-family: 'iconfont';  /* project id 257490 */
      src: url('//at.alicdn.com/t/font_whdchx2yj9bpgb9.eot');
      src: url('//at.alicdn.com/t/font_whdchx2yj9bpgb9.eot?#iefix') format('embedded-opentype'),
      url('//at.alicdn.com/t/font_whdchx2yj9bpgb9.woff') format('woff'),
      url('//at.alicdn.com/t/font_whdchx2yj9bpgb9.ttf') format('truetype'),
      url('//at.alicdn.com/t/font_whdchx2yj9bpgb9.svg#iconfont') format('svg');
    }
    .iconfont {
      font-family:"iconfont" !important;
      font-size:16px;
      font-style:normal;
      -webkit-font-smoothing: antialiased;
      -webkit-text-stroke-width: 0.2px;
      -moz-osx-font-smoothing: grayscale;
    }
    ul {
      margin: 0;
      padding: 0;
    }
    li {
      font-style: normal;
    }
    html, body {
      margin: 0;
      padding: 0
    }
  </style>
</head>
<body>
  <div>
    <ul>
      <% for (var item in htmlWebpackPlugin.options.openPage) { %>
        <li>
          <i class="iconfont">&#xee97;</i>
          <a href="<%=htmlWebpackPlugin.options.publicPath %><%=htmlWebpackPlugin.options.openPage[item] %>" title="button">
            <span class="name"><%=item %></span>
          </a>
        </li>
      <% } %>
      </ul>
  </div>
</body>
</html>